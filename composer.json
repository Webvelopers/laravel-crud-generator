{
    "name": "webvelopers/laravel-crud-generator",
    "authors": [{
        "name": "Webvelopers, Inc.",
        "email": "webmaster@webvelopers.net"
    }],
    "type": "library",
    "description": "Laravel CRUD Generator is a library, it implements a new command to create: model, migration, factory, seeder, request and controller(resources) files with operations, with aditional option to generate a full API Controller.",
    "keywords": [
        "library",
        "laravel",
        "CRUD",
        "Generator"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/database": "^10.0",
        "illuminate/console": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/testing": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.19"
    },
    "autoload": {
        "psr-4": {
            "Webvelopers\\LaravelCRUDGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webvelopers\\LaravelCRUDGenerator\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webvelopers\\LaravelCRUDGenerator\\LaravelCRUDGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}